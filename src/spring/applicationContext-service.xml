<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="userService" class="com.ant.education.service.impl.UserServiceImpl">
		<property name="userDao" ref="userDao"></property>
	</bean>

	<bean id="trainInfoService" class="com.ant.education.service.impl.TrainInfoServiceImpl">
		<property name="trainInfoDao" ref="trainInfoDao"></property>
	</bean>
	<bean id="menuTypeService" class="com.ant.education.service.impl.MenuTypeServiceImpl">
		<property name="menuTypeDao" ref="menuTypeDao"></property>
	</bean>
	<bean id="courseInfoService" class="com.ant.education.service.impl.CourseInfoServiceImpl">
		<property name="courseInfoDao" ref="courseInfoDao"></property>
	</bean>
	<bean id="teacherInfoService" class="com.ant.education.service.impl.TeacherInfoServiceImpl">
		<property name="teacherInfoDao" ref="teacherInfoDao"></property>
	</bean>
	<bean id="subjectService" class="com.ant.education.service.impl.SubjectServiceImpl">
		<property name="subjectDao" ref="subjectDao"></property>
	</bean>
	<bean id="subjectItemService" class="com.ant.education.service.impl.SubjectItemServiceImpl">
		<property name="subjectItemDao" ref="subjectItemDao"></property>
