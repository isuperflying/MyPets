	<bean id="userExamService" class="com.ant.education.service.impl.UserExamServiceImpl">
		<property name="userExamDao" ref="userExamDao"></property>
	</bean>
	<bean id="scoreItemService" class="com.ant.education.service.impl.ScoreItemServiceImpl">
		<property name="scoreItemDao" ref="scoreItemDao"></property>
	</bean>
	<bean id="petRecordService" class="com.ant.education.service.impl.PetRecordServiceImpl">
		<property name="petRecordDao" ref="petRecordDao"></property>
	</bean>
	<bean id="idiomInfoService" class="com.ant.education.service.impl.IDiomInfoServiceImpl">
